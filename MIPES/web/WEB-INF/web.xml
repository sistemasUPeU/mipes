<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>LoginController</servlet-name>
        <servlet-class>com.upeu.mipes.controller.LoginController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>RegESController</servlet-name>
        <servlet-class>com.upeu.mipes.controller.RegESController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>RegIntgController</servlet-name>
        <servlet-class>com.upeu.mipes.controller.RegIntgController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>IntegranteController</servlet-name>
        <servlet-class>com.upeu.mipes.controller.IntegranteController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>EscuelaController</servlet-name>
        <servlet-class>com.upeu.mipes.controller.EscuelaController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>MinisterioController</servlet-name>
        <servlet-class>com.upeu.mipes.controller.MinisterioController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>PersonaController</servlet-name>
        <servlet-class>com.upeu.mipes.controller.PersonaController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Asistencia</servlet-name>
        <servlet-class>com.upeu.mipes.controller.AsistenciaController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>MainController</servlet-name>
        <servlet-class>com.upeu.mipes.controller.MainController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DistritoController</servlet-name>
        <servlet-class>com.upeu.mipes.controller.DistritoController</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>LoginController</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>RegESController</servlet-name>
        <url-pattern>/reg=newES</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>RegIntgController</servlet-name>
        <url-pattern>/RegInt</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>IntegranteController</servlet-name>
        <url-pattern>/IntCnt</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>EscuelaController</servlet-name>
        <url-pattern>/esc</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>MinisterioController</servlet-name>
        <url-pattern>/min</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>PersonaController</servlet-name>
        <url-pattern>/regPer</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Asistencia</servlet-name>
        <url-pattern>/Asistencia</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>MainController</servlet-name>
        <url-pattern>/home</url-pattern>
    </servlet-mapping>
    <welcome-file-list>
        <welcome-file>/login.jsp</welcome-file>
    </welcome-file-list>
    <servlet-mapping>
        <servlet-name>DistritoController</servlet-name>
        <url-pattern>/distrito</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
